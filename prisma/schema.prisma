generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  createdAt DateTime @default(now())
  username  String   @unique
  password String @unique
  classes Classes[]
}

model Classes {
  name        String
  code        String @unique
  class       String
  description String
  id          String    @id @unique @default(uuid())
  madeById    String
  members      User[]
}